//LEADERS

#include<bits/stdc++.h>
void leaders(int arr[] , int n)
{  int m = arr[n-1];
  for(int i=n-2 ; i>=0;i--)
  {
    if(arr[i]>m)
    {
     cout<<arr[i];
    }
    m=max(m,arr[i);
  
  }


}
void main()
{ int size;
 cout<< "enter size of array";
 cin<< size;
 int *a = new int(size);
 for(int i=0 ;i<size;i++)
 {
   
  cin>>a[i];
 
 }
 leaders(a,size);
}



// pattern
#include<bits/stdc++.h>

using namespace std;

void main() {
	int n;
	cin>>n;
	
	for(int i=1 ; i<=n ;i++)
	{
	    for(int j=1 ; j<=2n ;j++)
	    {
	        if(j<=i || j>=2n+1-i )
	        {
	            cout<<"*";
	            
	        }
	        else
	        {
	            cout<<" ";
	        }
	        
	        
	        
	    }
	    cout<<endl;
	    
	    
	    
	}
	for(int i=n ; i>=1 ;i--)
	{
	    for(int j=1 ; j<=2n ;j++)
	    {
	        if(j<=i || j>=2n+1-i )
	        {
	            cout<<"*";
	            
	        }
	        else
	        {
	            cout<<" ";
	        }
	        
	        
	        
	    }
	    cout<<endl;
	    
	    
	    
	}
	
	
	
	
	
	
	
	
}
